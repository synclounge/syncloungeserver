{
  "name": "syncloungeserver",
  "version": "0.0.0-semantically-released",
  "description": "SyncLounge socket server",
  "bin": "./dist/index.js",
  "main": "./dist/lib.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "dev": "nodemon --exec babel-node src",
    "prepare": "if-env SKIP_BUILD=true && echo 'Skipping build' || npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/synclounge/syncloungeserver.git"
  },
  "keywords": [
    "synclounge"
  ],
  "author": {
    "name": "Travis Shivers"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/synclounge/syncloungeserver/issues"
  },
  "homepage": "https://github.com/synclounge/syncloungeserver#readme",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.7",
    "@babel/node": "^7.16.8",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@delucis/if-env": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "babel-preset-airbnb": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "nconf": "^0.11.3",
    "socket.io": "^4.4.1",
    "url-join": "^4.0.1",
    "uuid": "^8.3.2"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.6",
    "utf-8-validate": "^5.0.9"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=12"
  }
}
